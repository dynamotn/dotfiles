# vim:sw=4 sts=4 filetype=sshconfig
Host *
    HashKnownHosts yes
    Protocol 2
    StrictHostKeyChecking ask
    AddKeysToAgent yes
    ForwardX11 no
    HostbasedAuthentication no
    Ciphers -arcfour*,-*cbc

Host github.com
    IdentityFile ~/.ssh/key/personal_ed25519

Host gitlab.com
    IdentityFile ~/.ssh/key/personal_ed25519
